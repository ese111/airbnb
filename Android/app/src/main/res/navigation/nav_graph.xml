<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.team16.airbnb.ui.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/myBookFragment"
        android:name="com.team16.airbnb.ui.mybook.MyBookFragment"
        android:label="MyBookFragment" >
        <action
            android:id="@+id/action_myBookFragment_to_detailMyBookFragment"
            app:destination="@id/detailMyBookFragment" />
    </fragment>
    <fragment
        android:id="@+id/wishFragment"
        android:name="com.team16.airbnb.ui.wish.WishFragment"
        android:label="WishFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.team16.airbnb.ui.search.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/searchResultFragment"
        android:name="com.team16.airbnb.ui.search.SearchResultFragment"
        android:label="fragment_search_result"
        tools:layout="@layout/fragment_search_result" />
    <fragment
        android:id="@+id/detailMyBookFragment"
        android:name="com.team16.airbnb.ui.mybook.detail.DetailMyBookFragment"
        android:label="DetailMyBookFragment" />
</navigation>